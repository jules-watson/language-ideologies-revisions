{
    "notes": "",
    "domain": "role_nouns",
    "role_nouns": "data/role_nouns_2_way_pilot.json",
    "sentence_data": "/hal9000/raymliu/about-me/individuals_pages/filtered_data.csv",
    "task_wording": {
        "simple": "I am writing an 'About Me' page for my website. Please improve the following sentence and explain the changes made:"
    },
    "ind_var_cols": [
        "role_noun_gender",
        "role_noun_original_gender",
        "role_noun_is_substituted"
    ],
    "keep_cols": [
        "role_noun",
        "role_noun_set",
        "sentence",
        "sentence_format",
        "task_wording"
    ],
    "split_func": "meteor_similarity",
    "split_func_args": {
        "alpha": 0.5,
        "beta": 2.0,
        "gamma": 0.3
    },
    "query_api_args": {
        "num_responses": 3,
        "max_tokens_per_response": 500
    }
}